# Webshell API Configuration
# Copy this file to .env and update values

# CTFd Configuration
CTFD_URL=https://2k26-rsuctf.nulbytez.live

# Webshell Base URL (where ttyd instances are accessible)
WEBSHELL_BASE_URL=https://webshell.nullbytez.live

# Docker Configuration
CONTAINER_NETWORK=webshell-network
CONTAINER_IMAGE=webshell-instance:latest
CONTAINER_MEMORY_LIMIT=512m
CONTAINER_CPU_LIMIT=0.5
CONTAINER_TIMEOUT_HOURS=24

# API Configuration
PORT=5000
DEBUG=false

# Admin API Secret (change this!)
API_SECRET=change-me-to-a-secure-random-string
